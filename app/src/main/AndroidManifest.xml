<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samone.ScientificCalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_calcu"
        android:label="Scientific Calculator"
        android:roundIcon="@mipmap/ic_launcher_calcu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.samone.ScientificCalculator.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>